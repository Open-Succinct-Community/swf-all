source $HOME/.bash_aliases

toolpath=`dirname $0`
projpath=$toolpath/../
cd $projpath

if [ $# -eq 0 ] 
then 
    user=root
else
    user=$1
fi


bin/h2console <<EOF 2>/dev/null |cut -f1 -d '|' 
set-property sql-result-showheader 0
set-property sql-result-showfooter 0
select api_key from users where name = '$user' ;
set-property sql-result-showheader 1
set-property sql-result-showfooter 1
EOF
